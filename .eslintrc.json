{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": 0,
    "jsx-a11y/alt-text": 1,
    "react/jsx-key": 0,
    "no-unused-vars": 0,
    "valid-jsdoc": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
